@using T034.Models
@model Db.Entity.Subject

@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<div>@Model.Price</div>
<div>@Model.Description</div>

@Html.Partial("FileUploadPartialView", new FileUploadModel
    {
        Url = "UploadFile", 
        Category = "Subject", 
        Id = Model.Id.ToString()
    })